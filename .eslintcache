ation: null,\n      display: '0',\n    };\n    this.handleClick = this.handleClick.bind(this);\n  }\n\n  handleClick = name => {\n    const { total, next, operation } = this.state;\n    const result = Calculator({ total, next, operation }, name);\n    switch (name) {\n      case '=':\n        this.setState(state => ({\n          display: state.operation === null || state.next === null ? state.total : result.total,\n          operation: null,\n          next: null,\n          total: null,\n        }));\n        break;\n      case '+':\n      case '-':\n      case '*':\n      case '/':\n        this.setState(state => ({\n          ...result, total: state.display === '0' || state.display === 'syntax error' ? total : state.display, operation: state.display === 'syntax error' ? operation : name,\n        }));\n        break;\n      case 'AC':\n        this.setState({ ...result, display: '0' });\n        break;\n      default:\n        this.setState({ ...result, display: '0' });\n        break;\n    }\n  };\n\n  render() {\n    const { next, total, display } = this.state;\n    console.log(Calculator({ total: '3', next: '4', operation: '*' }, '='));\n    return (\n      <>\n        <Route exact path=\"/\">\n          <Home />\n        </Route>\n        <Route exact path=\"/quote\">\n          <Quote />\n        </Route>\n        <Route exact path=\"/calculator\">\n          <Navbar />\n          <div className=\"calc-body\">\n            <Display result={next === null ? total : next} display={display} />\n            <ButtonPanel clickBtn={this.handleClick} />\n          </div>\n        </Route>\n      </>\n    );\n  }\n}\n\nexport default App;\n","C:\\Rose - React\\React-Calculator\\src\\components\\Display.js",[],"C:\\Rose - React\\React-Calculator\\src\\components\\ButtonPanel.js",[],"C:\\Rose - React\\React-Calculator\\src\\components\\Home.js",[],"C:\\Rose - React\\React-Calculator\\src\\components\\Quote.js",[],"C:\\Rose - React\\React-Calculator\\src\\components\\Na